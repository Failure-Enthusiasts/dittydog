{
  "family": "dittydog",
  "networkMode": "awsvpc",
  "taskRoleArn": "",
  "executionRoleArn": "",
  "containerDefinitions": [
    {
      "name": "spotify-backend",
      "image": "bradleyjay/spotify-backend:aws_test_deploy",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080
        }
      ],
      "environment": [
        {
          "name": "FLASK_APP",
          "value": "backend.py"
        },
        {
          "name": "FLASK_ENV",
          "value": "development"
        },
        {
          "name": "FLASK_DEBUG",
          "value": "1"
        },
        {
          "name": "FLASK_RUN_PORT",
          "value": "8080"
        },
        {
          "name": "REDIS_URL",
          "value": "redis://redis:6385"
        }
      ],
      "linuxParameters": {
        "initProcessEnabled": true
      }
    },
    {
      "name": "middleware-server",
      "image": "bradleyjay/spotify-middleware:aws_test_deploy",
      "cpu": 100,
      "memory": 512,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 4001,
          "hostPort": 4001
        }
      ],
      "linuxParameters": {
        "initProcessEnabled": true
      }
    },
    {
      "name": "redis",
      "image": "redis:alpine",
      "cpu": 100,
      "memory": 512,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 6385,
          "hostPort": 6385
        }
      ],
      "linuxParameters": {
        "initProcessEnabled": true
      }
    }
  ],
  "requiresCompatibilities": [
    "EC2",
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512"
}